#  1. combine RNAseq and CUT&Tag --Diff  
##==========================================================
RNA =read.csv("RNAseq.csv")
Ribo=read.csv("Deseq2_all_peak.annotation.csv")

dim(RNA)
dim(Ribo)

## combineï¼›
combine= merge(RNA,Ribo,
               by.x="SYMBOL",
               by.y="SYMBOL",
               suffixes = c(".RNA",".Ribo") ,
               all.x=F,
               all.y=F)

dim(combine)

# save 
write.table(combine,"merge.txt",sep="\t",row.names=FALSE)
